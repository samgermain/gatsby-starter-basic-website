{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "gatsby develop",
        "type": "node",
        "request": "launch",
        "protocol": "inspector",
        "program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
        "args": ["develop"],
        "stopOnEntry": false,
        "runtimeArgs": ["--nolazy"],
        "sourceMaps": false
      },
      {
        "name": "gatsby build",
        "type": "node",
        "request": "launch",
        "protocol": "inspector",
        "program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
        "args": ["build"],
        "stopOnEntry": false,
        "runtimeArgs": ["--nolazy"],
        "sourceMaps": false
      }
    ]
  }